<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#5089dd"
     >

    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="20dp"
        android:layout_marginTop="20dp"
         >

            
    <com.example.shareholders.view.CircleMenuLayout
        android:id="@+id/id_menulayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_circle_layout" >

        <RelativeLayout
            android:id="@+id/circle_menu_item_center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <ImageView
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_centerInParent="true"
                android:padding="25dp"
                android:src="@drawable/menu_delu"
                android:background="@drawable/bg_circle_layout"
                 />
            
        </RelativeLayout>
        
    </com.example.shareholders.view.CircleMenuLayout>
        
        
        <TextView 
            android:id="@+id/tv_home_shareholder"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="40dp"
            android:gravity="center"
            android:text="STOCKHOLDER ASSEMBLAGE"
            android:textSize="14sp"
            android:textColor="#ffffff"
            />
    
    </LinearLayout>

</RelativeLayout>