<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#efeff4"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/rl_head"
            android:layout_width="match_parent"
            android:layout_height="@dimen/me_title_height"
            android:background="@color/white" >

            <com.example.shareholders.common.CircleImageView
                android:id="@+id/ic_face"
                android:layout_width="@dimen/figure_size"
                android:layout_height="@dimen/figure_size"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/figure_margin_left"
                android:src="@drawable/ico_default_headview" />

            <TextView
                android:id="@+id/tv_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/state_margin_left"
                android:layout_toRightOf="@+id/ic_face"
                android:text="未登录"
                android:textColor="#333333"
                android:textSize="15sp" />

            <ImageView
                android:id="@+id/iv_nav"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/nav_margin_right"
                android:src="@drawable/btn_tiaozhuan" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/me_ll_margin"
            android:background="@color/white"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/rl_information_center"
                android:layout_width="match_parent"
                android:layout_height="@dimen/me_item_height"
                android:paddingLeft="@dimen/figure_margin_left" >

                <ImageView
                    android:id="@+id/iv_information_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ico_xiaoxi1" />

                <TextView
                    android:id="@+id/tv_information_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/figure_margin_left"
                    android:layout_toRightOf="@+id/iv_information_center"
                    android:text="@string/information_center"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/iv_nav_information_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:src="@drawable/btn_tiaozhuan" />

                <TextView
                    android:id="@+id/iv_information"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="10sp"
                    android:text="5"
                    android:visibility="gone"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:layout_toLeftOf="@+id/iv_nav_information_center"
                    android:background="@drawable/bg_age" />
            </RelativeLayout>
            
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:paddingLeft="@dimen/me_line_margin"
                android:src="#eaeaea" />

            <RelativeLayout
                android:id="@+id/rl_my_survey"
                android:layout_width="match_parent"
                android:layout_height="@dimen/me_item_height"
                android:paddingLeft="@dimen/figure_margin_left" >

                <ImageView
                    android:id="@+id/iv_my_survey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ico_wodediaoyan11" />

                <TextView
                    android:id="@+id/tv_my_survey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/figure_margin_left"
                    android:layout_toRightOf="@+id/iv_my_survey"
                    android:text="@string/my_all_survey"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/iv_nav_my_survey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:src="@drawable/btn_tiaozhuan" />
            </RelativeLayout>

            
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/me_ll_margin"
            android:background="@color/white"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/rl_friend_list"
                android:layout_width="match_parent"
                android:layout_height="@dimen/me_item_height"
                android:paddingLeft="@dimen/figure_margin_left" >

                <ImageView
                    android:id="@+id/iv_friend_list"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ico_guyou" />

                <TextView
                    android:id="@+id/tv_friend_list"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/figure_margin_left"
                    android:layout_toRightOf="@+id/iv_friend_list"
                    android:text="@string/friend_list"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/iv_nav_friend_list"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:src="@drawable/btn_tiaozhuan" />
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:paddingLeft="@dimen/me_line_margin"
                android:src="#eaeaea" />

            <RelativeLayout
                android:id="@+id/rl_friend_moment"
                android:layout_width="match_parent"
                android:layout_height="@dimen/me_item_height"
                android:paddingLeft="@dimen/figure_margin_left" >

                <ImageView
                    android:id="@+id/iv_friend_moment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ico_guyoudongtai" />

                <TextView
                    android:id="@+id/tv_friend_moment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/figure_margin_left"
                    android:layout_toRightOf="@+id/iv_friend_moment"
                    android:text="@string/friend_moment"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/iv_nav_friend_moment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:src="@drawable/btn_tiaozhuan" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/me_ll_margin"
            android:background="@color/white"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/rl_collected_comments"
                android:layout_width="match_parent"
                android:layout_height="@dimen/me_item_height"
                android:paddingLeft="@dimen/figure_margin_left" >

                <ImageView
                    android:id="@+id/iv_collected_comments"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ico_pinglunshoucang" />

                <TextView
                    android:id="@+id/tv_collected_comments"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/figure_margin_left"
                    android:layout_toRightOf="@+id/iv_collected_comments"
                    android:text="@string/collected_comments"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/iv_nav_collected_comments"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:src="@drawable/btn_tiaozhuan" />
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:paddingLeft="@dimen/me_line_margin"
                android:src="#eaeaea" />

            <RelativeLayout
                android:id="@+id/rl_collected_consults"
                android:layout_width="match_parent"
                android:layout_height="@dimen/me_item_height"
                android:paddingLeft="@dimen/figure_margin_left" >

                <ImageView
                    android:id="@+id/iv_collected_consults"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ico_zixunshoucang" />

                <TextView
                    android:id="@+id/tv_collected_consults"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/figure_margin_left"
                    android:layout_toRightOf="@+id/iv_collected_consults"
                    android:text="@string/collected_consults"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/iv_nav_collected_consults"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:src="@drawable/btn_tiaozhuan" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/me_ll_margin"
            android:background="@color/white"
            android:orientation="vertical" >

            

            <RelativeLayout
                android:id="@+id/rl_voice_record"
                android:layout_width="match_parent"
                android:layout_height="@dimen/me_item_height"
                android:paddingLeft="@dimen/figure_margin_left" >

                <ImageView
                    android:id="@+id/iv_voice_record"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ico_record" />

                <TextView
                    android:id="@+id/tv_voice_record"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/figure_margin_left"
                    android:layout_toRightOf="@+id/iv_voice_record"
                    android:text="@string/voice_record"
                    android:textSize="15sp" />

                <ImageView
                    android:id="@+id/iv_nav_voice_record"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/nav_margin_right"
                    android:src="@drawable/btn_tiaozhuan" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

</ScrollView>